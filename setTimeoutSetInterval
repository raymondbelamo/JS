//setTimeout() calls a function after a specified amount of time
console.log("Hello")
setTimeout(() => {
  console.log("You There?")
}, 3000);

//setTimeout() using one liner arrow functions
console.log("Hello")
setTimeout(() => console.log("You There?") , 3000)



//setInterval() calls a function repeatedly after a specified amount of time
const id = setInterval(() => {
  console.log(Math.random())
}, 2000);

//run clearInterval(id); to stop function
