<script>
  let square = {
    area : function (x) {
        return x * x;
    },
    perimeter : function (x) {
        return x * 4;
    }
};

//Using arrow functions
  let square = {
    area : (x) => { return x * x; },
    perimeter : (x) => { return x * 4; }
};

//square.area(10) should return 100

//Egg Laying Exercise
  let hen = {
    name: 'Helen',
    eggCount: 0,
    layAnEgg() {
        this.eggCount++;
        return 'EGG';
    }
};

/* hen.name returns 'Helen'
   hen.eggCount returns 0
   hen.layAnEgg() returns 'EGG'
   hen.layAnEgg() returns 'EGG'
   hen.eggCount returns 2
</script>
